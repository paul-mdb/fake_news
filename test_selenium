from selenium import webdriver
from paragraphs import skip_cookie_popup


driver = webdriver.Firefox()
driver.get('https://fr.euronews.com/green/2021/07/14/tout-le-monde-scrute-le-methane')
skip_cookie_popup(driver, '#didomi-notice-agree-button')

content_elements = driver.find_elements_by_css_selector('.c-article-content')
for i,p in enumerate(content_elements) :
    print(i)
    print(p.text)

driver.quit()